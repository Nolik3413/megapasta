Tab:AddButton({
    Name = "–°–¥–∞—á–∞ –ø–∞—Ç—Ä–æ–Ω",
    Callback = function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace["\208\180\209\131\209\128\208\186\208\176"].a1.ammostoiki:GetChildren()[8]:GetChildren()[4].CFrame
    end
})

Tab:AddTextbox({
    Name = "–í–∑—è—Ç—å –Ω–∞—Ä—É—á–Ω–∏–∫–∞–º–∏ –∏–≥—Ä–æ–∫–∞",
    Default = "–ù–∏–∫ –∏–≥—Ä–æ–∫–∞",
    TextDisappear = true,
    Callback = function(txt)
        local player = game.Players:FindFirstChild(txt)
        if player then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
            wait(0.20)
            local Event = game:GetService("ReplicatedStorage").ArrestFunction
            Event:InvokeServer(player)

        else
            print("–ò–≥—Ä–æ–∫ —Å —Ç–∞–∫–∏–º –Ω–∏–∫–æ–º –Ω–µ –Ω–∞–π–¥–µ–Ω.")
        end
    end
})

Tab:AddTextbox({
    Name = "–£–±–∏—Ç—å –∏–≥—Ä–æ–∫–∞",
    Default = "–ù–∏–∫ –∏–≥—Ä–æ–∫–∞",
    TextDisappear = true,
    Callback = function(txt)
        local player = game.Players:FindFirstChild(txt)
        if player then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
            wait(0.20)
            local Event = game:GetService("ReplicatedStorage").ArrestFunction
            Event:InvokeServer(player)
            wait(1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.killpart.CFrame
            wait(0.20)
local A_1 = "\68\101\97\99\116\105\118\97\116\101"
local Event = game:GetService("ReplicatedStorage").ArrestFunction
Event:InvokeServer(A_1)

        else
            print("–ò–≥—Ä–æ–∫ —Å —Ç–∞–∫–∏–º –Ω–∏–∫–æ–º –Ω–µ –Ω–∞–π–¥–µ–Ω.")
        end
    end
})

--[[
Name = <string> - The name of the textbox.
Default = <string> - The default value of the textbox.
TextDisappear = <bool> - Makes the text disappear in the textbox after losing focus.
Callback = <function> - The function of the textbox.
]]
